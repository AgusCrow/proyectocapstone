            <hashTree/>
            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Extract User ID from JWT" enabled="true">
              <stringProp name="scriptLanguage">javascript</stringProp>
              <stringProp name="script">// Extract user ID and username from JWT token
              var token = vars.get(&quot;auth_token&quot;);
              if (token != null &amp;&amp; token != &quot;TOKEN_NOT_FOUND&quot;) {
                  try {
                      // JWT token is base64 encoded, split by . and get the payload (second part)
                      var parts = token.split(&quot;.&quot;);
                      if (parts.length &gt;= 2) {
                          var payload = parts[1];
                          // Base64 decode the payload
                          var decoded = java.lang.String(java.util.Base64.getDecoder().decode(payload));
                          // Parse the JSON payload
                          var json = JSON.parse(decoded);
                          var userId = json.id;
                          var username = json.username;
                          if (userId) {
                              vars.put(&quot;jwt_user_id&quot;, userId);
                              vars.put(&quot;player_id&quot;, userId); // Also set as player_id for consistency
                              log.info(&quot;Extracted user ID from JWT: &quot; + userId);
                          }
                          if (username) {
                              vars.put(&quot;jwt_username&quot;, username);
                              log.info(&quot;Extracted username from JWT: &quot; + username);
                          }
                      }
                  } catch (e) {
                      log.info(&quot;Error parsing JWT token: &quot; + e);
                  }
              } else {
                  log.info(&quot;Auth token not found&quot;);
              }</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">false</stringProp>
            </JSR223PostProcessor>
