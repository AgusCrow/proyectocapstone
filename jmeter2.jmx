            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Select Correct Token" enabled="true">
              <stringProp name="script">import org.apache.jmeter.threads.JMeterContext;

// Obtener los diferentes tokens extraídos
def tokenMain = vars.get(&quot;token_main&quot;);
def tokenAlt1 = vars.get(&quot;token_alt1&quot;);
def tokenAlt2 = vars.get(&quot;token_alt2&quot;);

// Seleccionar el primer token que no sea &quot;TOKEN_NOT_FOUND&quot;
def selectedToken = &quot;&quot;;

if (tokenMain != null &amp;&amp; !tokenMain.equals(&quot;TOKEN_NOT_FOUND&quot;) &amp;&amp; !tokenMain.isEmpty()) {
    selectedToken = tokenMain;
    log.info(&quot;Token seleccionado desde token_main: &quot; + selectedToken);
} else if (tokenAlt1 != null &amp;&amp; !tokenAlt1.equals(&quot;TOKEN_NOT_FOUND&quot;) &amp;&amp; !tokenAlt1.isEmpty()) {
    selectedToken = tokenAlt1;
    log.info(&quot;Token seleccionado desde token_alt1: &quot; + selectedToken);
} else if (tokenAlt2 != null &amp;&amp; !tokenAlt2.equals(&quot;TOKEN_NOT_FOUND&quot;) &amp;&amp; !tokenAlt2.isEmpty()) {
    selectedToken = tokenAlt2;
    log.info(&quot;Token seleccionado desde token_alt2: &quot; + selectedToken);
} else {
    log.error(&quot;No se encontró ningún token válido&quot;);
    selectedToken = &quot;NO_TOKEN_FOUND&quot;;
}

// Guardar el token seleccionado
vars.put(&quot;auth_token&quot;, selectedToken);

// Debug: mostrar todos los tokens en el log
log.info(&quot;Token Main: &quot; + tokenMain);
log.info(&quot;Token Alt1: &quot; + tokenAlt1);
log.info(&quot;Token Alt2: &quot; + tokenAlt2);
log.info(&quot;Selected Token: &quot; + selectedToken);</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">false</stringProp>
            </JSR223PostProcessor>
